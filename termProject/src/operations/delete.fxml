<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>


<AnchorPane prefHeight="140.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="operations.DeleteController">
   <children>
      <Label layoutX="42.0" layoutY="63.0" text="Type an employee's ID to delete its record:">
         <opaqueInsets>
            <Insets />
         </opaqueInsets>
      </Label>
      <TextField fx:id="textFieldID" layoutX="327.0" layoutY="59.0" promptText="empID" />
      <Button fx:id="buttonID" layoutX="231.0" layoutY="97.0" mnemonicParsing="false" onAction="#deleteEmp" text="Delete" />
      <Button fx:id="menuButton" layoutX="314.0" layoutY="97.0" mnemonicParsing="false" onAction="#goBack" text="Menu" />
   </children>
</AnchorPane>
