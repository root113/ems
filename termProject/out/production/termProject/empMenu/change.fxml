<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>


<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="empMenu.ChangeController">
   <children>
      <TextField fx:id="tfID" layoutX="226.0" layoutY="66.0" promptText="Your ID" />
      <PasswordField fx:id="pfNew1" layoutX="226.0" layoutY="158.0" promptText="New Password" />
      <PasswordField fx:id="pfNew2" layoutX="226.0" layoutY="200.0" promptText="Re-enter New Password" />
      <Button fx:id="btSave" layoutX="258.0" layoutY="259.0" mnemonicParsing="false" onAction="#savePassword" text="Save Change" />
      <Button fx:id="btMenu" layoutX="275.0" layoutY="305.0" mnemonicParsing="false" onAction="#goBack" text="Menu" />
   </children>
</AnchorPane>
